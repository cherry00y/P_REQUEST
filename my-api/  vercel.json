{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/Requestor/(.*)",
      "dest": "/routes/requestor.js"
    },
    {
      "src": "/Login/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/Admin/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/Operator/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/Dashboard/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    }
  ]
}
